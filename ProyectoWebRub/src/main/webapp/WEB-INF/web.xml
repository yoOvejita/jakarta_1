<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" id="WebApp_ID" version="2.5">
  <display-name>ProyectoWebRub</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Un simple controlador de ejemplo</description>
    <display-name>nombreController</display-name>
    <servlet-name>xyz</servlet-name>
    <servlet-class>com.proyecto.nombreController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>xyz</servlet-name>
    <url-pattern>/nombrecito</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>segundoServlet</display-name>
    <servlet-name>segundoServlet</servlet-name>
    <servlet-class>com.proyecto.segundoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>segundoServlet</servlet-name>
    <url-pattern>/segundo</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>coockiesController</display-name>
    <servlet-name>coockiesController</servlet-name>
    <servlet-class>com.proyecto.coockiesController</servlet-class>
    <init-param>
      <param-name>dato</param-name>
      <param-value>Anita</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>coockiesController</servlet-name>
    <url-pattern>/galletasGratis</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>dato</param-name>
    <param-value>Sofia</param-value>
  </context-param>
  <filter>
    <display-name>UnFiltro</display-name>
    <filter-name>UnFiltro</filter-name>
    <filter-class>com.filtros.UnFiltro</filter-class>
    <init-param>
      <param-name>enMantenimiento</param-name>
      <param-value>no</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>UnFiltro</filter-name>
    <url-pattern>/galletasGratis</url-pattern>
  </filter-mapping>
  <filter>
    <display-name>otroFiltro</display-name>
    <filter-name>otroFiltro</filter-name>
    <filter-class>com.filtros.otroFiltro</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>otroFiltro</filter-name>
    <url-pattern>/nombrecito</url-pattern>
  </filter-mapping>
</web-app>